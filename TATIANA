# Crear un arreglo bidimensional de 3x3
matriz = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]

# Acceder a un elemento específico
elemento = matriz[1][2]  # Accede al elemento en la fila 2, columna 3 (valor 6)
print("Elemento en la posición [1][2]:", elemento)

# Modificar un elemento específico
matriz[0][0] = 10  # Cambiar el valor en la fila 1, columna 1
print("Matriz después de modificar el primer elemento:")
for fila in matriz:
    print(fila)
